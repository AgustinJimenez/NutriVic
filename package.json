{
  "name": "app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npx react-native start",
    "s": "npx react-native start -- --reset-cache",
    "test": "jest",
    "link": "npx react-native link",
    "lint": "eslint src/",
    "prettify": "prettier --write 'src/**/*.js'",
    "android": "yarn run adb:reverse && npx react-native run-android",
    "a": "yarn run android",
    "android:production": "npx react-native run-android --variant=release",
    "a:p": "yarn run android:production",
    "android:install": "adb install -r -d android/app/build/outputs/apk/release/app-armeabi-v7a-release.apk",
    "a:i": "yarn run android:install",
    "android:run": "yarn run a:a && yarn run a:i",
    "a:r": "yarn run android:run",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "a:c": "yarn run android:clean",
    "android:assemble": "rm -rf android/app/build/outputs/apk/release/*.apk && cd android && ./gradlew assembleRelease && cd ..",
    "a:a": "yarn jetify && yarn run android:assemble",
    "android:bundle": "cd android &&./gradlew bundleRelease && cd ..",
    "a:b:b": "yarn run android:bundle",
    "android:build": "cd android && ./gradlew build --daemon && cd ..",
    "a:b": "yarn run android:build",
    "android:build:clean": "cd android && ./gradlew clean build --daemon && cd ..",
    "a:b:c": "yarn run android:build:clean",
    "ios": "npx react-native run-ios",
    "i": "yarn run ios",
    "ios:simulator": "npx react-native run-ios",
    "i:s": "yarn run ios:simulator",
    "ios:device": "npx react-native run-ios --device",
    "i:d": "yarn run ios:device",
    "ios:production": "npx react-native run-ios --configuration Release",
    "i:p": "yarn run ios:production",
    "ios:device:production": "npx react-native run-ios --configuration Release --device",
    "i:d:p": "yarn run ios:device:production",
    "pod:update": "cd ios && pod update && cd ..",
    "pod:install": "npx pod-install ios",
    "p:i": "yarn pod:install",
    "i:p:i": "yarn run ios:pod:install",
    "ios:pod:update": "cd ios && pod update && cd ..",
    "i:p:u": "yarn run ios:pod:install",
    "backend": "node backendTestServer.js",
    "manage:native:files": "node docs/nativeFilesManager.js",
    "m:n:f": "node docs/nativeFilesManager.js",
    "cache:clear": "rm -rf /tmp/metro-bundler-cache-* && rm -rf /tmp/haste-map-react-native-packager-* && watchman watch-del-all && npm cache verify",
    "c:c": "yarn run cache:clear",
    "rebuild": "yarn run c:c && rm -rf node_modules package-lock.json && yarn && npx react-native link && cd ios && pod deintegrate && pod install && cd ..",
    "r": "yarn run rebuild",
    "adb:reverse": "adb reverse tcp:8081 tcp:8081",
    "lint:fix": "eslint src/ --fix",
    "generate:keystore": "cd android/app && keytool -genkeypair -v -keystore my-upload-key.keystore -alias my-key-alias -keyalg RSA -keysize 2048 -validity 10000 -file deployment_cert.der && cd ..",
    "g:k": "yarn generate:keystore"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.6.2",
    "@react-native-community/datetimepicker": "^2.4.2",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/netinfo": "^4.4.0",
    "@react-native-community/picker": "^1.8.1",
    "@react-native-community/push-notification-ios": "^1.0.3",
    "@react-navigation/drawer": "^5.8.4",
    "@react-navigation/native": "^5.6.1",
    "@react-navigation/stack": "^5.6.2",
    "axios": "^0.19.0",
    "collect.js": "^4.18.9",
    "http-status": "^1.5.0",
    "http-status-codes": "^1.4.0",
    "i18next": "^17.2.0",
    "moment": "^2.28.0",
    "native-base": "^2.13.14",
    "react": "16.13.1",
    "react-axios": "^2.0.4",
    "react-i18next": "^10.13.1",
    "react-native": "0.63.2",
    "react-native-animatable": "^1.3.3",
    "react-native-background-fetch": "^2.7.1",
    "react-native-calendars": "^1.300.0",
    "react-native-device-info": "^5.6.1",
    "react-native-easy-grid": "^0.2.2",
    "react-native-geolocation-service": "^5.0.0",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-global-font": "^1.0.2",
    "react-native-keyboard-aware-scroll-view": "^0.9.3",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-maps": "0.26.1",
    "react-native-modal": "^11.5.6",
    "react-native-modal-datetime-picker": "^8.7.1",
    "react-native-permissions": "^2.1.5",
    "react-native-picker-select": "^8.0.2",
    "react-native-push-notification": "^3.1.9",
    "react-native-reanimated": "^1.9.0",
    "react-native-safe-area-context": "^3.0.7",
    "react-native-screens": "^2.9.0",
    "react-native-search-bar": "^3.5.1",
    "react-native-svg": "^12.1.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-swipe-list-view": "^2.0.3",
    "react-native-vector-icons": "^7.1.0",
    "react-redux": "^7.1.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.1",
    "styled-components": "^5.2.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "@types/react": "^16.9.13",
    "@types/react-native": "^0.60.23",
    "@types/react-native-push-notification": "^3.0.7",
    "@types/react-redux": "^7.1.5",
    "@types/react-test-renderer": "^16.9.1",
    "@types/styled-components": "^5.1.4",
    "@types/uuid": "^3.4.6",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react-hooks": "^2.3.0",
    "express": "^4.17.1",
    "ip": "^1.1.5",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-native-json-tree": "^1.2.0",
    "react-test-renderer": "16.13.1",
    "xml2js": "^0.4.22"
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "jest": {
    "preset": "react-native"
  }
}
